def find_winner(N):
    if N % 6 == 0:
        return "Mahesh"
    else:
        return "Dinesh"

# Read the number of test cases
T = int(input())

# Process each test case
for _ in range(T):
    # Read the value of N for each test case
    N = int(input())

    # Find and print the winner for each test case
    print(find_winner(N))
